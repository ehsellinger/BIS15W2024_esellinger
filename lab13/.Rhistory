map2 <- get_stadiamap(bbox1, maptype = "stamen_terrain_lines", zoom=7)
ggmap(map2)
ggmap(map2) +
geom_point(data = wolves_restricted, aes(long, lat), size=0.4) +
labs(x= "Longitude", y= "Latitude", title = "Wolf Populations")
ggmap(map2) +
geom_point(data = wolves_restricted, aes(long, lat, color = pop), size=4) +
labs(x= "Longitude", y= "Latitude", title = "Wolf Populations")
ggmap(map2) +
geom_point(data = wolves_restricted, aes(long, lat, color = pop), size=4) +
labs(x= "Longitude",
y= "Latitude",
title = "Wolf Populations",
color = "Population")
ggmap(map2) +
geom_point(data = wolves_restricted, aes(long, lat, color = pop, fill = pop), size=4) +
labs(x= "Longitude",
y= "Latitude",
title = "Wolf Populations",
color = "Population")
ggmap(map2) +
geom_point(data = wolves_restricted, aes(long, lat, color = pop), size=4) +
labs(x= "Longitude",
y= "Latitude",
title = "Wolf Populations",
color = "Population")
